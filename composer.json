{
    "name": "gtlogistics/quickbase-client",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Gtlogistics\\QuickbaseClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gtlogistics\\QuickbaseClient\\Test\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Brandon Antonio Lorenzo",
            "email": "bantonio@gtglobal.com"
        }
