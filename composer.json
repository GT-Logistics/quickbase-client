{
    "name": "gtlogistics/quickbase-client",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Gtlogistics\\QuickbaseClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gtlogistics\\QuickbaseClient\\Test\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Brandon Antonio Lorenzo",
            "email": "bantonio@gtglobal.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "symfony/polyfill-php80": "^1.28",
        "symfony/polyfill-php81": "^1.28",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "webmozart/assert": "^1.11",
        "thecodingmachine/safe": "^1.3|^2.0|^3.0",
        "php-http/client-common": "^2.7",
        "halaxa/json-machine": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^v8.18.0",
        "phpunit/phpunit": "^8.5|^10.1",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "thecodingmachine/phpstan-safe-rule": "^1.3",
        "illuminate/support": "^5.8|^10.0",
        "mockery/mockery": "^1.4",
        "nyholm/psr7": "^1.8",
        "symfony/http-client": "^5.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gtlogistics\\QuickbaseClient\\Bridge\\Laravel\\QuickbaseClientServiceProvider"
            ]
        }
    }
}
